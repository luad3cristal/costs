{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "MY_ENV_VAR": "value"
  },
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "startCommand": "npm run backend"
}
